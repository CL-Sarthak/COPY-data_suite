{
  "name": "cirrus-data-suite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:db": "npm run db:start && npm run dev",
    "dev:clean": "npm run db:reset && npm run dev",
    "prebuild": "node scripts/set-build-info.js && node scripts/run-migrations.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:start": "docker-compose up -d postgres",
    "db:stop": "docker-compose down",
    "db:reset": "tsx scripts/reset-database.ts",
    "db:reset:docker": "docker-compose down -v && docker-compose up -d postgres && sleep 3",
    "db:logs": "docker-compose logs -f postgres",
    "db:shell": "docker-compose exec postgres psql -U postgres -d data_redaction_dev",
    "migrate": "node scripts/run-migrations.js",
    "migrate:check": "DATABASE_URL=postgres://dummy node scripts/run-migrations.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=4096' jest --coverage",
    "test:all": "jest --testPathIgnorePatterns=[]",
    "test:unit": "jest",
    "test:coverage:components": "jest src/components --coverage",
    "test:coverage:services": "jest src/services --coverage",
    "test:coverage:utils": "jest src/utils --coverage",
    "test:smoke": "tsx scripts/run-smoke-tests.ts",
    "test:smoke:ci": "jest --config jest.smoke.config.js --ci",
    "test:coverage:fast": "jest --coverage --collectCoverageFrom='src/components/**/*.{ts,tsx}' --collectCoverageFrom='src/utils/**/*.{ts,tsx}' --collectCoverageFrom='src/services/**/*.{ts,tsx}'",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=4096' jest --ci --coverage --watchAll=false",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:integration:coverage": "jest --config jest.integration.config.js --coverage",
    "test:smoke:local": "tsx scripts/run-smoke-tests.ts http://localhost:3000",
    "cleanup-datasources": "npx tsx src/scripts/cleanup-large-datasources.ts",
    "db:validate": "npx tsx scripts/validate-database-schema.ts",
    "db:test-compatibility": "npm run db:validate && npm run build",
    "reapply-mappings": "npx tsx src/scripts/reapply-field-mappings.ts",
    "smoke": "node scripts/smoke-tests.js",
    "smoke:local": "node scripts/smoke-tests.js http://localhost:3000",
    "smoke:prod": "node scripts/smoke-tests.js https://cirrus-ai-data-suite.vercel.app",
    "test:db:start": "cd docker/test-databases && docker compose up -d",
    "test:db:stop": "cd docker/test-databases && docker compose down",
    "test:db:clean": "cd docker/test-databases && docker compose down -v",
    "test:db:logs": "cd docker/test-databases && docker compose logs -f test-postgres-medical",
    "test:db:shell": "cd docker/test-databases && docker compose exec test-postgres-medical psql -U medical_admin -d medical_records",
    "test:db:connector": "tsx scripts/test-database-connector.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.51.0",
    "@aws-sdk/client-s3": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.826.0",
    "@faker-js/faker": "^9.8.0",
    "@heroicons/react": "^2.2.0",
    "@monaco-editor/react": "^4.7.0",
    "@types/uuid": "^10.0.0",
    "@vercel/blob": "^1.1.1",
    "lucide-react": "^0.511.0",
    "mysql2": "^3.14.1",
    "next": "15.3.2",
    "pdfjs-dist": "^5.2.133",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "reactflow": "^11.11.4",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/faker": "^6.6.8",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axios": "^1.10.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "next-test-api-route-handler": "^4.0.16",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
